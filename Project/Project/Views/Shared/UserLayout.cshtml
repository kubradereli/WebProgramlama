
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="zxx">

<head>
    <title>Mavii Etkinlik</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="utf-8">
    <meta name="keywords" content="Weblog a Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <link rel="shortcut icon" href="~/UserImageFiles/mavii.png" />

    <script>
        addEventListener("load", function () {
            setTimeout(hideURLbar, 0);
        }, false);

        function hideURLbar() {
            window.scrollTo(0, 1);
        }
    </script>
    <link href="~/ProjeTema/css/bootstrap.css" rel='stylesheet' type='text/css' />
    <link rel="stylesheet" href="~/ProjeTema/css/single.css">
    <link href="~/ProjeTema/css/style.css" rel='stylesheet' type='text/css' />
    <link href="~/ProjeTema/css/fontawesome-all.css" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800"
          rel="stylesheet">
</head>

<body>
    <header>
        <div class="top-bar_sub_w3layouts container-fluid">
            <div class="row">
                <div class="col-md-4 logo text-left">
                    <a class="navbar-brand" href="/Activity/Index/">
                        <i class="far fa-star" style="color:#3399ff"></i> MAVİİ
                    </a>
                </div>
                @await Html.PartialAsync("Partial2")
            </div>
        </div>
        @await Html.PartialAsync("Partial1")
    </header>
    @RenderBody()
    @await Html.PartialAsync("Partial3")
    <script src="~/ProjeTema/js/jquery-2.2.3.min.js"></script>
    <script src="~/ProjeTema/js/move-top.js"></script>
    <script src="~/ProjeTema/js/easing.js"></script>
    <script>
		jQuery(document).ready(function ($) {
			$(".scroll").click(function (event) {
				event.preventDefault();
				$('html,body').animate({
					scrollTop: $(this.hash).offset().top
				}, 900);
			});
		});
    </script>
    <script>
		$(document).ready(function () {
			/*
									var defaults = {
							  			containerID: 'toTop', // fading element id
										containerHoverID: 'toTopHover', // fading element hover id
										scrollSpeed: 1200,
										easingType: 'linear'
							 		};
									*/
			$().UItoTop({
				easingType: 'easeOutQuart'
			});
		});
    </script>
    <a href="#home" class="scroll" id="toTop" style="display: block;">
        <span id="toTopHover" style="opacity: 1;"> </span>
    </a>
    <script src="~/ProjeTema/js/bootstrap.js"></script>
</body>
</html>